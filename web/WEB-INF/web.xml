<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>A</servlet-name>
        <servlet-class>test.A</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SupplierReg</servlet-name>
        <servlet-class>servlet.SupplierReg</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UnitSave</servlet-name>
        <servlet-class>servlet.UnitSave</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProductReg</servlet-name>
        <servlet-class>servlet.ProductReg</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Grn</servlet-name>
        <servlet-class>servlet.Grn</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Vehiclesave</servlet-name>
        <servlet-class>servlet.Vehiclesave</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>A</servlet-name>
        <url-pattern>/A</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SupplierReg</servlet-name>
        <url-pattern>/SupplierReg</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UnitSave</servlet-name>
        <url-pattern>/UnitSave</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProductReg</servlet-name>
        <url-pattern>/ProductReg</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Grn</servlet-name>
        <url-pattern>/Grn</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Vehiclesave</servlet-name>
        <url-pattern>/Vehiclesave</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
